				</div>
			</div>
		</div>
	</section>
	<section>
		<div class="container">
			<h1 class="text-center">FORGOT OTP VERIFICATION</h1>
		</div>
	</section>
	<!-- End Banner Area -->

	<!--================Login Box Area =================-->
	<section class="login_box_area section_gap">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<div class="login_form_inner" style="padding-top: 0px;">
						<h3>Verify the OTP Check Your Email</h3>

						<form class="row login_form" action="/verify-forgot-otp" method="post" onsubmit="return otpValidate()"
							novalidate="novalidate">
							<div class="col-md-12 form-group">
								<% if (typeof message !=='undefined' ) { %>
									<div class="alert alert-danger" role="alert">
										<%= message %>
									</div>
									<% } %>
										<br>
										<input type="text" class="form-control" id="otp" name="otp" placeholder="Enter your otp">
										<p style="color: red;" id="otpError"></p>
							</div>
							<div class="col-md-12 form-group">
								<button type="submit" value="submit" class="primary-btn">Verify</button>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!--================End Login Box Area =================-->

